<!-- student_marks.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Marks</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
    <h1>Your Marks</h1>

    <nav>
        <a href="/student/dashboard" class="back-link">Back to Dashboard</a>
    </nav>

    <table>
        <thead>
            <tr>
                <th>Subject</th>
                <th>Marks</th>
            </tr>
        </thead>
        <tbody>
            <% marks.forEach(subject => { %>
            <tr>
                <td><%= subject.subject %></td>
                <td><%= subject.marks %></td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
